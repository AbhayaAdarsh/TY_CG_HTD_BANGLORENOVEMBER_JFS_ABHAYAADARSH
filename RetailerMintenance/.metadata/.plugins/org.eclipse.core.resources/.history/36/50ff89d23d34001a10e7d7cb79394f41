package com.capgemini.retailermaintenancesystemapplication.dto;

public class RetailerBean {

	@Entity
	@Table(name="retailer_db")
	public class ProductBean {
		@Id
		@Column
		@GeneratedValue
		private int id;
		@Column(unique =true,nullable =false)
		private String imageUrl;
		@Column
		private String name;
		@Column
		private double cost;
		
