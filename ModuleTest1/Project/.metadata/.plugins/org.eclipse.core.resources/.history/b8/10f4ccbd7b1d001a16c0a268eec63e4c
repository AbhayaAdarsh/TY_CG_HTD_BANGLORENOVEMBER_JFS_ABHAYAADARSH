package com.cape.phonesimulator;

public class AllContact {
	
	int name;
	int number;
	String group;
	
	public AllContact(String name2, int number, String group) {
		super();
		this.name = name2;
		this.number = number;
		this.group = group;
	}
	
	public int getName() {
		return name;
	}
	public void setName(int name) {
		this.name = name;
	}
	public int getNumber() {
		return number;
	}
	public void setNumber(int number) {
		this.number = number;
	}
	public String getGroup() {
		return group;
	}
	public void setGroup(String group) {
		this.group = group;
	}
	
	public static AllContact createAllContact(String name,int number,String group)
	{
		return new AllContact(name,number,group);
	}
	
	@Override
	public String toString() {
		return "AllContact [name=" + name + ", number=" + number + ", group=" + group + "]";
	}
	

	public static void main(String[] args) {
		
	}
